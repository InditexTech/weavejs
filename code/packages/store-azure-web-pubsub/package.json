{
  "name": "@inditextech/weavejs-store-azure-web-pubsub",
  "version": "0.17.0-SNAPSHOT",
  "type": "module",
  "license": "Apache-2.0",
  "author": "Jesus Manuel Piñeiro Cid <jesusmpc@inditex.com>",
  "maintainers": [
    {
      "name": "Jesus Manuel Piñeiro Cid",
      "email": "jesusmpc@inditex.com"
    }
  ],
  "exports": {
    "import": "./dist/store-azure-web-pubsub.js",
    "require": "./dist/store-azure-web-pubsub.cjs"
  },
  "types": "dist/store-azure-web-pubsub.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:snapshot": "vite build --mode=development --sourcemap --emptyOutDir",
    "build": "vite build --mode=production --minify --emptyOutDir",
    "bump:snapshot": "npm version $npm_package_version.$(date \"+%s\")",
    "bundle:analyze": "vite-bundle-visualizer",
    "check": "echo \"Monorepo test script\" && exit 0",
    "dev": "vite build --watch",
    "dev:server": "tsx --env-file=.env ./server/server.ts",
    "dist:tag": "npm dist-tag add \"$(jq -r .name package.json)@$(jq -r .version package.json)\" \"$PR_TAG\" --registry=\"$NPM_PUBLISHING_REGISTRY\" --verbose",
    "format": "npm run lint -- --quiet --fix --fix-type layout",
    "link": "npm link",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint ./src",
    "publish:snapshot": "npm publish",
    "release:perform": "npm publish --access public",
    "release:prepare": "npm run verify",
    "start": "tsx ./bin/server.ts",
    "test": "vitest --passWithNoTests",
    "types:check": "tsc --noEmit",
    "verify": "npm run lint && npm run test && npm run build",
    "version:development": "npm version $(npm version minor)-SNAPSHOT",
    "version:release": "npm version $RELEASE_VERSION -m \"[npm-scripts] prepare release $RELEASE_VERSION\" --tag-version-prefix \"\""
  },
  "devDependencies": {
    "@azure/web-pubsub": "1.1.4",
    "@azure/web-pubsub-express": "^1.0.6",
    "@inditextech/weavejs-types": "0.17.0-SNAPSHOT",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "@vitest/coverage-v8": "1.6.0",
    "@vitest/ui": "1.6.0",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "eslint": "8.57.1",
    "express": "^4.21.2",
    "globals": "15.14.0",
    "js-base64": "^3.7.7",
    "lib0": "^0.2.49",
    "reconnecting-websocket": "^4.4.0",
    "tsx": "^4.19.3",
    "typescript-eslint": "8.22.0",
    "uuid": "^11.1.0",
    "vite": "5.2.9",
    "vite-bundle-visualizer": "1.1.0",
    "vite-plugin-compression2": "1.0.0",
    "vite-plugin-dts": "4.0.3",
    "vitest": "1.6.0",
    "vitest-sonar-reporter": "2.0.0",
    "ws": "^8.18.1",
    "y-protocols": "1.0.6"
  },
  "engines": {
    "node": "^18.12 || ^20.11 || ^22.11",
    "npm": ">= 8.19.x"
  },
  "nx": {
    "implicitDependencies": [
      "@inditextech/weavejs-types"
    ]
  }
}
