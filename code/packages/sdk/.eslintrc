{
  "env": {
    "es2022": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "globals": {
    "window": "readonly",
    "document": "readonly"
  },
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn"
  }
}
