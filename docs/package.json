{
  "name": "docs",
  "version": "0.14.2",
  "private": true,
  "scripts": {
    "build": "next build",
    "cleanup": "rm package-lock.json && npkill -D -y",
    "dev": "next dev --turbo",
    "git:pre-push": "npm run build",
    "release:prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset": "nx reset",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "verify": "npm ci --force && npm run build",
    "version:development": "export BUMP_DEVELOP_VERSION=$(npm version $(npm version minor)-prerelease)",
    "version:release": "npm ci --force && npm version $RELEASE_VERSION"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.0",
    "@react-spring/web": "^9.7.5",
    "framer-motion": "^12.9.2",
    "fumadocs-core": "15.3.3",
    "fumadocs-mdx": "11.6.4",
    "fumadocs-ui": "15.3.3",
    "lucide-react": "^0.511.0",
    "mermaid": "^11.6.0",
    "next": "15.3.1",
    "ogl": "^1.0.11",
    "raw-loader": "^4.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tw-animate-css": "^1.2.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^8",
    "eslint-config-next": "15.3.1",
    "npkill": "^0.12.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3"
  }
}
